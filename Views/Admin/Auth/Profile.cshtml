@model RestaurantBooking.Models.Admin

@{
    ViewData["Title"] = "Profil";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Profil</h1>

<div>
    
    <div class="row">
        <div class="col-12">
            <dl class="row">

                <dt class="col-sm-4">
                    Nama Lengkap
